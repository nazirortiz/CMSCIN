class Column {
    private $datafield;
    private $headertext;

    public function  __construct($datafield, $headertext) {
        $this->datafield = $datafield;
        $this->headertext = $headertext;
    }

    public function DataField(){
        return $this->datafield;
    }

    public function HeaderText(){
        return $this->headertext;
    }
}
